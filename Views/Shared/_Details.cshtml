@model ExamsManagement.Models.eexmmaster
<div class="modal-header">
    @*<button type="button" data-dismiss="modal" aria-hidden="true" id="closbtn"><span style="float:left;" class="fa fa-close"></span></button>*@
    <h4 class="modal-title"><b>بيانات الملف</b></h4>
</div>




<div class="modal-body">
    <div class="form-row">
        <div class="form-group col-md-6">
            <label for="inputtext4">المحافظة</label>
            <input type="text" class="form-control" readonly value="@Model.eegov">
        </div>
        <div class="form-group col-md-6">
            <label for="inputPassword4">المنطقة</label>
            <input type="text" class="form-control" readonly value="@Model.eedusection">
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
            <label for="inputtext4">المدرسة</label>
            <input type="text" class="form-control" readonly value="@Model.eschool">
        </div>
        <div class="form-group col-md-6">
            <label for="inputPassword4">الدور</label>
            <input type="text" class="form-control" readonly value="@Model.elevel">
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
            <label for="inputtext4">الحالة</label>
            <input type="text" class="form-control" readonly value="@Model.estatus">
        </div>
        <div class="form-group col-md-6">
            <label for="inputPassword4">السنه</label>
            <input type="text" class="form-control" readonly value="@Model.eyear">
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
            <label for="inputtext4">الشعبة</label>
            <input type="text" class="form-control" readonly value="@Model.eeduppx0">
        </div>
        <div class="form-group col-md-6">
            <label for="inputPassword4">الرقم المرجعى للملف</label>
            <input type="text" class="form-control" readonly value="@Model.edocref">
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
            <label for="inputtext4">الصفحه</label>
            <input type="text" class="form-control" readonly value="@Model.epage">
        </div>
        <div class="form-group col-md-6">
            <label for="inputPassword4">الرقم المرجعى للصفحة</label>
            <input type="text" class="form-control" readonly value="@Model.erowref">
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
            <label for="inputPassword4">من رقم الصفحه</label>
            <input type="text" class="form-control" readonly value="@Model.estudentfrom">
        </div>
        <div class="form-group col-md-6">
            <label for="inputtext4">الي رقم الصفحه</label>
            <input type="text" class="form-control" readonly value="@Model.estudentto">
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
            <label for="inputtext4">الشهادة</label>
            <input type="text" class="form-control" readonly value="@Model.eedulictype">
        </div>
        <div class="form-group col-md-6">
            <label for="inputPassword4">الكنترول</label>
            <input type="text" class="form-control" readonly value="@Model.eeCount">
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
            <label for="inputPassword4">المستخدم</label>
            <input type="text" class="form-control" readonly value="@ViewBag.Username">
        </div>
        <div class="form-group col-md-6">
            <label for="inputtext4">ملف الحفظ متاح</label>
            @if (Model.DocThere == true)
            {
                <input type="checkbox" checked disabled />
            }
            else
            {
                <input type="checkbox" disabled />
            }
        </div>

    </div>


</div>

    <div class="modal-footer">
        <div class="col-md-12">
            <input class="btn green" value="غلق" type='button' id="closeBtn2"/>
        </div>

    </div>

   
    <script>

        $("#closeBtn2").click(function () {
            $("#myModal").modal('hide');

        });


        
    </script>

